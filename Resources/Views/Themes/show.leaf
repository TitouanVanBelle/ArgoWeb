#set("body") {
  <form method="POST" action="/themes/#(id)/update">
    <table>
      #for(word in words) {
        <tr>
          <td>
            <input type="text" value="#(word)" name="words[]" class="form-control">
          </td>
        </tr>
      }
      <tr>
        <td>
          <input class="btn btn-blue" type="submit" value="Save">
        </td>
      </tr>
    </table>
  </form>
}

#embed("nav_layout")
