#set("body") {
  <ul>
  #for(package in packages) {
    <li>
      <a href="/packages/#(package.id)">
        <div class="card">
          <div class="container">
            <h4>#(package.name) (#(package.numberOfCards))</h4>
          </div>
        </div>
      </a>
    </li>
  }
  </ul>
  #if(user.admin) {
    <div id="toolbar">
      <a href="packages/new">
        <button class="btn btn-blue" type="button">New</button>
      </a>
    </div>
  }
}

#embed("nav_layout")
