#set("body") {
    <div id="content-body">
      <form method="POST" action="/packages">
        <input type="text" placeholder="Name" name="name" class="form-control" maxlength="30">
        <input type="text" placeholder="Tag" name="tag" class="form-control">
        <input type="text" placeholder="Description" name="description" class="form-control" maxlength="45">
        <input class="btn btn-blue" type="submit" value="Create">
      </form>
    </div>
}

#embed("nav_layout")
