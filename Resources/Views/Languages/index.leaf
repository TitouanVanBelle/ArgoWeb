#set("body") {
  <ul>
  #for(language in languages) {
    <li>
      <a href="/languages/#(language.id)">
        <div class="card">
          <div class="container">
            <h4>#(language.name)</h4>
          </div>
        </div>
      </a>
    </li>
  }
  </ul>
  #if(user.admin) {
    <a href="languages/new">
      <button class="btn btn-blue" type="button">New</button>
    </a>
  }
}

#embed("nav_layout")
